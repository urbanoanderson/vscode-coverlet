{
    "name": "vscode-coverlet",
    "displayName": "Vscode Coverlet",
    "description": "Coverlet tools for Vscode",
    "icon": "img/icon-512.png",
    "version": "0.9.0",
    "publisher": "urbanoanderson",
    "homepage": "https://github.com/urbanoanderson/vscode-coverlet/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/urbanoanderson/vscode-coverlet.git"
    },
    "engines": {
        "vscode": "^1.32.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "Coverlet",
        "Test",
        "Coverage",
        "Report"
    ],
    "activationEvents": [
        "onCommand:extension.coverletStrip"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.coverletStrip",
                "title": "Coverlet Strip"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "tslint ./src/*.ts",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^6.0.40",
        "tslint": "^5.16.0",
        "typescript": "^3.5.1",
        "@types/vscode": "^1.32.0"
    }
}
